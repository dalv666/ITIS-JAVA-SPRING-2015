<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
	   					   http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean class="su.dalv.itis.java.spring.task2.ShakespearQuoter" name="shakespearQuoter">
        <property name="message" value="2b |!2b"></property>
    </bean>

    <bean class="su.dalv.itis.java.spring.task2.TerminatorQuoter" name="terminatorQuoter">
        <property name="messages">
            <list>
                <value>I'll be back</value>
                <value>Astalavista, baby</value>
            </list>
        </property>
    </bean>

</beans>
